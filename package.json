  "scripts": {
    "start": "pnpm build && cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon src/server.ts",
    "build": "tsc --build && tsc-alias",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "(prettier --config .prettierrc --check . || pnpm format) && eslint .",
    "format": "prettier --config .prettierrc --cache --write .",
    "prepare": "husky",
    "generate:dev": "cross-env NODE_ENV=development drizzle-kit generate --config=drizzle.config.ts",
    "generate:prod": "cross-env NODE_ENV=production drizzle-kit generate --config=drizzle.config.ts",
    "migrate:dev": "cross-env NODE_ENV=development drizzle-kit migrate --config=drizzle.config.ts",
    "migrate:prod": "cross-env NODE_ENV=production drizzle-kit migrate --config=drizzle.config.ts",
    "introspect:dev": "cross-env NODE_ENV=development drizzle-kit introspect --config=drizzle.config.ts",
    "introspect:prod": "cross-env NODE_ENV=production drizzle-kit introspect --config=drizzle.config.ts",
    "push:dev": "cross-env NODE_ENV=development drizzle-kit push --config=drizzle.config.ts",
    "push:prod": "cross-env NODE_ENV=production drizzle-kit push --config=drizzle.config.ts"
  },
  "lint-staged": {
    "**/*": "prettier --config .prettierrc --cache --write --ignore-unknown"
